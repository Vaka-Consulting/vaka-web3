    "@aws-sdk/client-sqs": "^3.470.0",
    "@apollo/server": "^4.9.3",
    "@blockfrost/blockfrost-js": "^5.4.0",
    "@emurgo/cardano-serialization-lib-nodejs": "^14.1.1",
    "@graphql-tools/graphql-file-loader": "^8.0.0",
    "@graphql-tools/load": "^8.0.0",
