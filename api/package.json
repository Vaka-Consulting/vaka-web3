{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "author": "Narayan Maharjan <narayan@vaka.consulting>",
  "scripts": {
    "compile": "tsc",
    "deploy": "sls deploy",
    "offline": "sls offline --stage dev",
    "create-domain:dev": "sls create_domain --stage dev",
    "create-domain:prod": "sls create_domain --stage prod",
    "start": "npm run compile && node ./dist/dev.js",
    "start:processor": "ts-node ./src/local.ts",
    "start:dev": "npx nodemon --exec 'node' --no-warnings=ExperimentalWarning  --loader ts-node/esm  ./src/dev.ts",
    "start:direct": "npx tsx src/dev.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "engines": {
    "node": ">=18.0.0 <20.0.0"
  },
